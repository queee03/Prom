{
  "name": "prom-ui",
  "version": "0.1.5",
  "private": false,
  "description": "React components library",
  "author": "Kuku",
  "license": "MIT",
  "keywords": [
    "Component",
    "UI",
    "React"
  ],
  "homepage": "https://github.com/queee03/Prom",
  "repository": {
    "type": "git",
    "url": "https://github.com/queee03/Prom"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.es.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "react-app-rewired start",
    "eject": "react-scripts eject",
    "test": "react-app-rewired test",
    "test:nowatch": "cross-env CI=true react-app-rewired test",
    "lint": "eslint --ext .js,.ts,.tsx src",
    "clean": "rimraf ./dist",
    "build": "npm run clean && npm run build:rollup",
    "build:ts": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "build:css": "node-sass ./src/styles/index.scss ./dist/index.css",
    "build:rollup": "rollup --config rollup.config.mjs",
    "storybook": "start-storybook -p 6006 -s public",
    "build:storybook": "build-storybook -s public",
    "prepublishOnly": "npm run test:nowatch && npm run lint && npm run build"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1",
      "axios": "axios/dist/node/axios.cjs"
    }
  },
  "peerDependencies": {
    "react": ">=17.0.2",
    "react-dom": ">=17.0.2"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.2.1",
    "@fortawesome/free-regular-svg-icons": "^6.2.1",
    "@fortawesome/free-solid-svg-icons": "^6.2.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "async-validator": "^4.2.5",
    "axios": "^1.3.4",
    "classnames": "^2.3.1",
    "lodash": "^4.17.21",
    "react-transition-group": "^4.4.5",
    "web-vitals": "^1.1.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-replace": "^5.0.2",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/node-logger": "^6.5.16",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^13.5.0",
    "@trivago/prettier-plugin-sort-imports": "^4.0.0",
    "@types/classnames": "^2.3.1",
    "@types/mdx": "^2.0.3",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/react-transition-group": "^4.4.5",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "babel-plugin-named-exports-order": "^0.0.2",
    "cross-env": "^7.0.3",
    "customize-cra": "^1.0.0",
    "eslint": "^8.33.0",
    "eslint-config-ali": "^14.0.2",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-storybook": "^0.6.10",
    "husky": "^8.0.0",
    "node-sass": "^8.0.0",
    "prettier": "^2.8.3",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-app-rewired": "^2.2.1",
    "react-dom": "^17.0.2",
    "react-scripts": "^5.0.1",
    "rimraf": "^5.0.0",
    "rollup": "^3.23.0",
    "rollup-plugin-exclude-dependencies-from-bundle": "^1.1.23",
    "rollup-plugin-sass": "^1.12.19",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.34.1",
    "stylelint-config-ali": "^0.4.2",
    "tsc-alias": "^1.8.6",
    "tslib": "^2.5.3",
    "typescript": "^4.9.5",
    "webpack": "^5.75.0"
  }
}
